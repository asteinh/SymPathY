[run]
branch = True
source =
    sympathor
omit =
    sympathor/__init__.py
    tests/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
directory = coverage_html
